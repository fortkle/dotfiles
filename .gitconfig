[core]
	editor = vim
	excludesfile = /Users/fortkle/.gitignore_global
[user]
	name = fortkle
	email = fortkle@gmail.com
[alias]
	st = status
	ch = checkout
	br = branch
	fe = fetch --prune
[pull]
	rebase = false
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
